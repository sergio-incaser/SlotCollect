<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.incaser.apps.slotcollect" >

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="es.incaser.apps.slotcollect.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="es.incaser.apps.slotcollect.Settings"
            android:label="Configuracion" />
        <activity
            android:name="es.incaser.apps.slotcollect.EstablecimientosLogic"
            android:label="@string/title_activity_establecimientos_logic" />
        <activity
            android:name="es.incaser.apps.slotcollect.DetallesEstablecimiento"
            android:label="@string/title_activity_detalles_establecimiento" />
        <activity
            android:name="es.incaser.apps.slotcollect.RecaudacionMaquina"
            android:label="@string/title_activity_recaudacion_maquina" />
        <activity
            android:name="es.incaser.apps.slotcollect.Recaudacion"
            android:label="@string/title_activity_Screen_Slide_Recaudacion" />
        <activity
            android:name="es.incaser.apps.slotcollect.Prestamos"
            android:label="@string/title_activity_prestamos" />
        <activity
            android:name="es.incaser.apps.slotcollect.RecuperacionesPrestamo"
            android:label="Recuperaciones Prestamo" />
        <activity
            android:name="es.incaser.apps.slotcollect.Incidencias"
            android:label="Incidencias" />
        <activity
            android:name="es.incaser.apps.slotcollect.IncidenciaEdit"
            android:label="Incidencia" />
        <activity
            android:name="es.incaser.apps.slotcollect.RecuperacionPrestamoEdit"
            android:label="Recuperaciones" />
        <activity
            android:name="es.incaser.apps.slotcollect.TotalesRecaudacion"
            android:label="@string/title_activity_totales_recaudacion" >
        </activity>
    </application>

</manifest>
